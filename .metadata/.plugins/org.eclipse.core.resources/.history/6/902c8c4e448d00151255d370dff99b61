package chapter_12;

public class Lab_5 {
	String fwrd, swrd;
	
	public Lab_5(String f, String s)
	{
		fwrd = f;
		swrd = s;
	}
	
	public void getA()
	{
		if(fwrd.length() > swrd.length()) System.out.println(fwrd + " has more letters than " + swrd);
		else if(fwrd.length() < swrd.length()) System.out.println(fwrd + " has less letters than " + swrd);
		else System.out.println(fwrd + " has the same amount of letters than " + swrd);
	}
	
	public static void main(String args[])
	{
		Lab_5 lb =  new Lab_5("hello", "goodbye");
		lb.getA();
	}
}
