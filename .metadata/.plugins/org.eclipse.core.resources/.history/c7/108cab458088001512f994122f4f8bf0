package chapter_10;

public class Lab_1 {
	
	public static String getFirstChunk( String line )
	{
        int a=line.indexOf(" ");
        String b;
        b = line.substring(0,a);
		return b;
	}

   public static void main( String args[] )
   {
		System.out.println( getFirstChunk("hello world") );   
		System.out.println( getFirstChunk("jim bob") );   
		System.out.println( getFirstChunk("sally sue") );  
		System.out.println( getFirstChunk("computer science") );  
		System.out.println( getFirstChunk("uil contests") );
   }
}
