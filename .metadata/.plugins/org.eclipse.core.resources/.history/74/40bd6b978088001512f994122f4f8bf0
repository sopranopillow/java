package chapter_10;

public class Lab_1 {
	
	public static String getFirst( String line )
	{
        int a=line.indexOf(" ");
        String b;
        b = line.substring(0,a);
		return b;
	}
	
	public static String getSecond( String line )
	{
        int a=line.indexOf(" ");
        String b;
        b = line.substring(0,a);
		return b;
	}

   public static void main( String args[] )
   {
		System.out.println("First: " +  getFirst("hello world") 
		+ "Second: " +  getSecond("hello world") 
		+ "Sum: hello world");   
		System.out.println("First: " +  getFirst("jim bob") 
		+ "Second: " +  getSecond("jim bob")
		+ "Sum: jim bob");   
		System.out.println("First: " +  getFirst("sally sue") 
		+ "Second: " +  getSecond("sally sue")
		+ "Sum: sally sue");  
		System.out.println("First: " +  getFirst("computer science") 
		+ "Second: " +  getSecond("computer science")
		+ "Sum: computer science");  
		System.out.println("First: " +  getFirst("uil contests") 
		+ "Second: " +  getSecond("uil contests")
		+ "Sum: uil contests");
   }
}
