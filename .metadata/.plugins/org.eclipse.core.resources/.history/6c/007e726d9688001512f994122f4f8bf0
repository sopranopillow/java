package chapter_10;

public class Lab_2 
{
	public static String getFirst( String line )
	{
        int a=line.indexOf(" ");
        String b;
        b = line.substring(0,a);
		return b;
	}

   public static void main( String args[] )
   {
		System.out.println("\nFirst: " +  getFirst("hello world"));
   }
}
